{
    "presets": ["env", "react", "stage-0", "stage-1"],
    "plugins": [
        "transform-decorators-legacy",
        "transform-class-properties",
        "syntax-dynamic-import", ["import", {
            "libraryName": "antd",
            "style": true
        }],
        [
            "transform-runtime", {
                "helpers": false,
                "polyfill": false,
                "regenerator": true,
                "moduleName": "babel-runtime"
            }
        ]
    ]
}